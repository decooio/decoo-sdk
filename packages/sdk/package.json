{
  "name": "@decooio/sdk",
  "version": "0.0.3",
  "description": "Decoo SDK",
  "keywords": [
    "decoo",
    "ipfs",
    "crust"
  ],
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "main": "src/index.js",
  "types": "dist/src/index.d.ts",
  "eslintConfig": {},
  "repository": {
    "type": "git",
    "url": "git@github.com:decooio/decoo-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "directory": "dist"
  },
  "scripts": {
    "build": "aegir build && cp package.json ./dist/package.json && cp README.md ./dist/README.md",
    "lint": "aegir lint",
    "coverage": "npx nyc -r html npm run test:node -- --bail",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check -i aegir -i rimraf"
  },
  "devDependencies": {
    "aegir": "^33.0.0",
    "rimraf": "^3.0.2"
  },
  "gitHead": "ca49d7b6bfd342e4929aac13d65c8824e69f41a9"
}
